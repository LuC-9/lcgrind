{
  "name": "lcgrind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "seed:companies": "tsx prisma/seed/seedCompanies.ts",
    "seed:all": "concurrently \"tsx prisma/seed/seedCompanies.ts\" \"tsx prisma/seed/seedLeetcode.ts\" && tsx prisma/seed/seedProblems.ts",
    "seed:leetcode": "tsx prisma/seed/seedLeetcode.ts",
    "seed:problems": "tsx prisma/seed/seedProblems.ts",
    "seed:sync": "cd .data && git pull && cd .. && tsx prisma/seed/seedProblems.ts",
    "generate": "prisma generate"
  }
}
